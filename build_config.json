    ".dwg", ".dxf", ".max", ".3ds", ".blend", ".skp",
    ".pem", ".p12", ".pfx", ".key", ".crt", ".cer",
    ".wallet", ".dat", ".log", ".cfg", ".ini", ".conf",
    ".bak", ".backup", ".old", ".tmp", ".temp",
    ".vmdk", ".vdi", ".vhd", ".ova", ".ovf",
    ".m4a", ".aac", ".ogg", ".wma", ".opus",
    ".webm", ".ogv", ".3gp", ".f4v", ".m4v",
